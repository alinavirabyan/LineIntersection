A = (1, 1)
B = (4, 4)
C = (1, 8)
D = (2, 4)

a1 = B[1] - A[1]
b1 = A[0] - B[0]
c1 = a1 * A[0] + b1 * A[1]

a2 = D[1] - C[1]
b2 = C[0] - D[0]
c2 = a2 * C[0] + b2 * C[1]

determinant = a1 * b2 - a2 * b1

if determinant == 0:
    print("The given lines AB and CD are parallel.")
else:
    x = (b2 * c1 - b1 * c2) / determinant
    y = (a1 * c2 - a2 * c1) / determinant
    
    print(f"The intersection of the given lines AB and CD is: ({x}, {y})")
